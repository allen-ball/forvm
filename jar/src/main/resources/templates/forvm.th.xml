<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id$ -->
<thlogic>
  <attr sel="head">
    <attr sel="link"
          th:each="css :
                     ${new String[] {
                         '/webjars/prismjs/themes/prism-tomorrow.css',
                         '/webjars/prismjs/plugins/toolbar/prism-toolbar.css',
                         '/webjars/prismjs/plugins/autolinker/prism-autolinker.css',
                         '/webjars/prismjs/plugins/command-line/prism-command-line.css',
                         '/webjars/prismjs/plugins/line-highlight/prism-line-highlight.css',
                         '/webjars/prismjs/plugins/line-numbers/prism-line-numbers.css',
                         '/webjars/prismjs/plugins/previewers/prism-previewers.css',
                         '/webjars/prismjs/plugins/unescaped-markup/prism-unescaped-markup.css',
                         '/webjars/prismjs/plugins/wpd/prism-wpd.css',
                         '/webjars/bootstrap/css/bootstrap.css'
                       }}"
          th:href="@{${css}}"/>
    <attr sel="style" th:text="'
body { padding-top: 60px; margin-bottom: 60px; }
@media (max-width: 979px) { body { padding-top: 0px; } }
'"/>
    <attr sel="script"
          th:each="script :
                     ${new String[] {
                         '/js/plugins.js'
                       }}"
          th:src="@{${script}}" th:text="''"/>
  </attr>
  <attr sel="body">
    <attr sel="/header"
          th:classappend="'fixed-top text-white bg-dark navbar navbar-dark'">
      <attr sel="/nav" th:classappend="'container navbar-expand-md'"/>
      <attr sel="nav/a[0]" th:classappend="'navbar-brand'"/>
      <attr sel="nav/ul" th:classappend="'navbar-nav text-white bg-dark'"/>
      <attr sel="nav/ul/li" th:classappend="'navbar-item dropdown'"/>
      <attr sel="nav/ul/li/a" th:classappend="'btn nav-link'"/>
      <attr sel="nav//ul/li/button"
            th:classappend="'btn dropdown-toggle text-white bg-dark'"
            th:id="${#ids.seq('dropdown')}"
            th:data-toggle="'dropdown'"
            th:aria-haspopup="true" th:aria-expanded="false"/>
      <attr sel="nav//ul/li/ul"
            th:classappend="'dropdown-menu text-white bg-dark'"
            th:aria-labelledby="${#ids.prev('dropdown')}"/>
      <attr sel="nav//ul/li/ul/li" th:classappend="'nav-item dropdown'"/>
      <attr sel="nav//ul/li/ul/li/a"
            th:classappend="'btn nav-link dropdown-item text-white bg-dark'"/>
    </attr>
    <attr sel="main" th:classappend="'container'">
      <attr sel="/table" th:classappend="'table table-striped'">
        <attr sel="/tbody">
          <attr sel="/tr" th:classappend="'row'"/>
          <attr sel="article-row">
            <attr sel="/td[0]" th:classappend="'col-sm'">
              <attr sel="/a" th:classappend="'row'"/>
            </attr>
          </attr>
          <attr sel="author-row">
            <attr sel="/td[0]" th:classappend="'col-sm-2 text-left'"/>
            <attr sel="/td[1]" th:classappend="'col-sm'"/>
          </attr>
        </attr>
      </attr>
    </attr>
    <attr sel="/footer" th:classappend="'fixed-bottom text-white bg-dark'">
      <attr sel="/nav" th:classappend="'container navbar navbar-dark'">
        <attr sel="/span[0]" th:classappend="'col-sm text-left'"/>
        <attr sel="/span[1]" th:classappend="'col-sm-3 text-center'"/>
        <attr sel="/span[2]" th:classappend="'col-sm text-right'"/>
      </attr>
      <attr sel="nav//a" th:classappend="'btn btn-link'"/>
    </attr>
    <attr sel="script"
          th:each="script :
                     ${new String[] {
                         '/webjars/jquery/jquery.js',
                         '/webjars/bootstrap/js/bootstrap.js',
                         '/webjars/prismjs/prism.js',
                         '/webjars/prismjs/components/prism-markup.js',
                         '/webjars/prismjs/components/prism-css.js',
                         '/webjars/prismjs/components/prism-clike.js',
                         '/webjars/prismjs/components/prism-javascript.js',
                         '/webjars/prismjs/components/prism-apacheconf.js',
                         '/webjars/prismjs/components/prism-c.js',
                         '/webjars/prismjs/components/prism-asciidoc.js',
                         '/webjars/prismjs/components/prism-csharp.js',
                         '/webjars/prismjs/components/prism-bash.js',
                         '/webjars/prismjs/components/prism-bison.js',
                         '/webjars/prismjs/components/prism-brainfuck.js',
                         '/webjars/prismjs/components/prism-cpp.js',
                         '/webjars/prismjs/components/prism-clojure.js',
                         '/webjars/prismjs/components/prism-ruby.js',
                         '/webjars/prismjs/components/prism-css-extras.js',
                         '/webjars/prismjs/components/prism-diff.js',
                         '/webjars/prismjs/components/prism-markup-templating.js',
                         '/webjars/prismjs/components/prism-erlang.js',
                         '/webjars/prismjs/components/prism-git.js',
                         '/webjars/prismjs/components/prism-groovy.js',
                         '/webjars/prismjs/components/prism-http.js',
                         '/webjars/prismjs/components/prism-ini.js',
                         '/webjars/prismjs/components/prism-java.js',
                         '/webjars/prismjs/components/prism-javastacktrace.js',
                         '/webjars/prismjs/components/prism-json.js',
                         '/webjars/prismjs/components/prism-kotlin.js',
                         '/webjars/prismjs/components/prism-latex.js',
                         '/webjars/prismjs/components/prism-markdown.js',
                         '/webjars/prismjs/components/prism-lisp.js',
                         '/webjars/prismjs/components/prism-lua.js',
                         '/webjars/prismjs/components/prism-makefile.js',
                         '/webjars/prismjs/components/prism-nginx.js',
                         '/webjars/prismjs/components/prism-perl.js',
                         '/webjars/prismjs/components/prism-php.js',
                         '/webjars/prismjs/components/prism-php-extras.js',
                         '/webjars/prismjs/components/prism-sql.js',
                         '/webjars/prismjs/components/prism-properties.js',
                         '/webjars/prismjs/components/prism-python.js',
                         '/webjars/prismjs/components/prism-sass.js',
                         '/webjars/prismjs/components/prism-scheme.js',
                         '/webjars/prismjs/components/prism-yaml.js',
                         '/webjars/prismjs/plugins/toolbar/prism-toolbar.js',
                         '/webjars/prismjs/plugins/autolinker/prism-autolinker.js',
                         '/webjars/prismjs/plugins/autoloader/prism-autoloader.js',
                         '/webjars/prismjs/plugins/command-line/prism-command-line.js',
                         '/webjars/prismjs/plugins/copy-to-clipboard/prism-copy-to-clipboard.js',
                         '/webjars/prismjs/plugins/custom-class/prism-custom-class.js',
                         '/webjars/prismjs/plugins/data-uri-highlight/prism-data-uri-highlight.js',
                         '/webjars/prismjs/plugins/file-highlight/prism-file-highlight.js',
                         '/webjars/prismjs/plugins/highlight-keywords/prism-highlight-keywords.js',
                         '/webjars/prismjs/plugins/jsonp-highlight/prism-jsonp-highlight.js',
                         '/webjars/prismjs/plugins/keep-markup/prism-keep-markup.js',
                         '/webjars/prismjs/plugins/line-highlight/prism-line-highlight.js',
                         '/webjars/prismjs/plugins/line-numbers/prism-line-numbers.js',
                         '/webjars/prismjs/plugins/normalize-whitespace/prism-normalize-whitespace.js',
                         '/webjars/prismjs/plugins/previewers/prism-previewers.js',
                         '/webjars/prismjs/plugins/remove-initial-line-feed/prism-remove-initial-line-feed.js',
                         '/webjars/prismjs/plugins/show-language/prism-show-language.js',
                         '/webjars/prismjs/plugins/unescaped-markup/prism-unescaped-markup.js',
                         '/webjars/prismjs/plugins/wpd/prism-wpd.js'
                       }}"
                       th:src="@{${script}}" th:text="''"/>
  </attr>
</thlogic>
